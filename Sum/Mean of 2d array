#include<stdio.h>

int main() {
    
    float myArray[3][3] = { { 0.1, 0.2, 0.3 }, { 1.1, 1.2, 1.3 }, { 2.1, 2.2, 2.3 } };
    int i, j;
    float sum, mean;
    
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            sum = sum + myArray[i][j];            
        }
    }
    printf("\nTotal sume is %.1f\n", sum);
    
    mean = sum / 9;
    printf("\nThe mean is %.1f", mean);
    
    return 0;
}
